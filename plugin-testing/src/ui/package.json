{
  "name": "testing-plugin-ui",
  "version": "1.0.0",
  "description": "UI for Testing Plugin Reports",
  "main": "launcher.js",
  "homepage": "./",
  "dependencies": {
    "axios": "^1.3.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "multer": "^2.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "recharts": "^3.7.0",
    "sqlite3": "^5.1.7",
    "unzipper": "^0.12.3"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  },
  "scripts": {
