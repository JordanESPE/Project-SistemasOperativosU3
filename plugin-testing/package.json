  "description": "Plugin modular para pruebas funcionales, no funcionales, carga y estrÃ©s",
  "main": "src/ui/report-server.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node src/ui/report-server.js",
    "dev": "nodemon src/ui/report-server.js",
    "build": "cd src/ui && npm install && npm run react-build",
    "postinstall": "cd src/ui && npm install",
    "plugin:all": "node src/plugin/core/executor.js --all",
    "plugin:functional": "node src/plugin/core/executor.js --functional",
    "plugin:non-functional": "node src/plugin/core/executor.js --non-functional",
    "plugin:load": "node src/plugin/core/executor.js --load",
    "plugin:stress": "node src/plugin/core/executor.js --stress",
    "plugin:report": "node src/plugin/core/executor.js --report",
    "ui:server": "node src/ui/report-server.js",
    "ui:launch": "node src/ui/report-server.js & start http://localhost:3002",
    "ui:electron": "electron src/ui/electron-main.js",
    "test": "jest",
    "db:init": "node src/backend/db/init.js"
  },
  "keywords": [
    "testing",
    "plugin",
    "functional",
    "non-functional",
    "load-testing",
    "stress-testing"
